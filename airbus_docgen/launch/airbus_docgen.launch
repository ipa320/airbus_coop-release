<?xml version="1.0"?>
<launch>
  <arg name="output_path" default="/tmp"/>
  <arg name="ros_pkg"/>

  <node pkg="airbus_docgen" type="index.py" name="airbus_docgen" output="screen">
    <param name="output_path" value="$(arg output_path)"/>
    <param name="pkg_dir" value="$(arg ros_pkg)"/>
  </node>

</launch>
